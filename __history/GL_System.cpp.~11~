//---------------------------------------------------------------------------

#pragma hdrstop

#include "GL_System.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)


TGLSystem::TGLSystem()
{
   name = L"GorizontLab Monitoring System";
   mark = L"...";
   node = NULL;
}

TGLSystem::~TGLSystem()
{

}


int TGLSystem::add_obj(WideString nm, TTreeNode* nd, int nn)
{
	TGLObject* ob = new TGLObject(nm, nd, nn);

	if (obj_list.add(ob) != 0)
	{
		return -1;
	}
	else
	{
		return 0;
    }


}


