#pragma hdrstop

#include "GL_List.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)

template <typename T>
TGLList<T>::TGLList()
{

}

template <typename T>
TGLList<T>::~TGLList()
{

}

template <typename T>
int TGLList<T>::add(T* obj)
{
   	m_list.push_back(obj);
	return 0;
}


